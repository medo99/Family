		<splash src="res/screen/android/screen-hdpi-portrait.png" platform="android" density="port-hdpi" />
		<splash src="res/screen/android/screen-xhdpi-portrait.png" platform="android" density="port-xhdpi" />
		<splash src="res/screen/android/screen-xxhdpi-portrait.png" platform="android" density="port-xxhdpi" />
		<splash src="res/screen/android/screen-xxxhdpi-portrait.png" platform="android" density="port-xxxhdpi" />
